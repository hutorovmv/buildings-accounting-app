@using BuildingsAccounting.Web.Models
@using BuildingsAccounting.Web.Helpers
@using BuildingsAccounting.Web.Pagination
@model BuildingsAccounting.Web.Pagination.PagedList<BuildingTypeTableModel>

<div class="container pb-4">
    <div class="row d-flex justify-content-start align-items-stretch">
        @foreach (var m in Model.Items)
        {
            @Html.Partial("_BuildingTypeCard", m)
        }
    </div>
    <div class="d-flex justify-content-center">
        @Html.Pagination(Model)
    </div>
</div>